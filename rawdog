from __future__ import print_function

from rawdoglib.rawdog import main

import os
import sys

def launch():
	sys.exit(main(sys.argv[1:]))

if __name__ == "__main__":
	if os.getenv("RAWDOG_PROFILE") is not None:
		import profile
		profile.run("launch()")
	else:
		launch()
